{
  "name": "@antoniogiroz/vue-skeleton",
  "type": "module",
  "version": "0.1.2",
  "description": "A Vue 3 component to create loading skeletons quickly",
  "author": "Antonio Giroz <antonioluisgil@gmail.com> (https://antoniogiroz.com)",
  "license": "MIT",
  "homepage": "https://github.com/antoniogiroz/vue-skeleton",
  "repository": {
    "type": "git",
    "url": "https://github.com/antoniogiroz/vue-skeleton"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "vue",
    "loading",
    "skeleton",
    "progress",
    "spinner"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p type-check build-only",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": ">=3.x.x"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.1.0",
    "typescript": "~4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-css-injected-by-js": "^3.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.7",
    "vue-tsc": "^1.2.0"
  }
}
