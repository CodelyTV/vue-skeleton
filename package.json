{
  "name": "@antoniogiroz/vue-skeleton",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Antonio Giroz",
  "license": "MIT",
  "homepage": "https://github.com/antoniogiroz/vue-skeleton",
  "repository": {
    "type": "git",
    "url": "https://github.com/antoniogiroz/vue-skeleton"
  },
  "keywords": [
    "vue",
    "loading",
    "skeleton",
    "progress",
    "spinner"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "run-p type-check build-only",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": ">=3.x.x"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "typescript": "~4.9.5",
    "vite": "^4.1.2",
    "vite-plugin-css-injected-by-js": "^3.0.1",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.3"
  }
}
